{
  "name": "play-away",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "pre-commit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "prisma:seed": "ts-node prisma/seed.ts",
    "postinstall": "prisma generate",
    "backup:create": "npx ts-node scripts/backup-database.ts",
    "backup:daily": "npx ts-node scripts/daily-backup.ts",
    "db:seed": "node scripts/seed-database.js",
    "db:seed:approve": "node scripts/seed-database.js --auto-approve",
    "db:approve-clubs": "node scripts/approve-seeded-clubs.js",
    "backup:test": "npx ts-node scripts/backup-database.ts --test",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@aws-sdk/client-s3": "^3.828.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@next/third-parties": "^15.5.11",
    "@prisma/client": "^6.9.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/blob": "^2.0.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^5.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.18.1",
    "lucide-react": "^0.525.0",
    "mapbox-gl": "^3.13.0",
    "next": "^15.3.8",
    "next-auth": "^5.0.0-beta.28",
    "node-fetch": "^3.3.2",
    "prisma": "^6.9.0",
    "react": "^19.0.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-map-gl": "^7.1.9",
    "resend": "^4.6.0",
    "schema-dts": "^1.1.5",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.1",
    "@react-email/preview-server": "^5.2.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20.19.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.1",
    "msw": "^2.10.2",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
